<h1 align="center">Review for <%= @destination.name %><img class="header-logo" src="/travelbug2_hollow_black.svg" width="15px"></h1>

<hr>

  By: <%= @rating.user.name %> <br>
  Comment: <%= @rating.note %> <br>
  Average Rating: <%= @rating.stars %> <br>

  <br><br>

  <% if @rating.user == current_user %>
    <%= link_to "Edit Destination Review", edit_destination_rating_path(@destination, @rating)%>
  <% end %>
